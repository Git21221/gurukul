{
  "private": true,
  "name": "gurukul-fullstack-app",
  "version": "1.0.0",
  "author": "Saikat Das",
  "description": "A fullstack application for Gurukul project.",
  "type": "module",
  "scripts": {
    "dev:client:main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/main dev:client:main",
    "dev:client:payment-main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/payment-main dev:client:payment-main",
    "dev:client:brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/brand-app/brand dev:client:brand",
    "dev:client:payment-brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/brand-app/payment-brand dev:client:payment-brand",
    "dev:server:main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/main dev:server:main",
    "dev:server:payment-main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/payment-main dev:server:payment-main",
    "dev:server:brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/brand-app/brand dev:server:brand",
    "dev:server:payment-brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/payment-brand dev:server:payment-brand",
    "build:client:main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/main build:client:main",
    "build:client:payment-main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/payment-main build:client:payment-main",
    "build:client:brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/brand-app/brand build:client:brand",
    "build:client:payment-brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/brand-app/payment-brand build:client:payment-brand",
    "build:server:main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/main build:server:main",
    "build:server:payment-main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/payment-main build:server:payment-main",
    "build:server:brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/brand-app/brand build:server:brand",
    "build:server:payment-brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/server/payment-brand build:server:payment-brand",
    "preview:client:main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/main preview:client:main",
    "preview:client:payment-main": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/brand-app/payment-main preview:client:payment-main",
    "preview:client:brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/brand-app/brand preview:client:brand",
    "preview:client:payment-brand": "pnpm install && pnpm --filter ./gurukul/gurukul-apps/client/brand-app/payment-brand preview:client:payment-brand",
    "lint": "eslint .",
    "test": "jest --passWithNoTests --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-amplify": "^3.750.0",
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/s3-request-presigner": "^3.806.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@tailwindcss/vite": "^4.1.8",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esm": "^3.2.25",
    "express": "^4.21.2",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-snippet": "^1.1.9",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.1.0",
    "lucide-react": "^0.513.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.13.0",
    "mongoose": "^8.10.0",
    "multer": "1.4.5-lts.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "sass": "^1.89.1",
    "simple-git": "^3.27.0",
    "stream": "^0.0.3",
    "tailwindcss": "^4.1.8",
    "tmp": "^0.2.3",
    "util": "^0.12.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^24.0.0",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "nodemon": "^3.1.9",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "vite": "^6.1.0"
  }
}
